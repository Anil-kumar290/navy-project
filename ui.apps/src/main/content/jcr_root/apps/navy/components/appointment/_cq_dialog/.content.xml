<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="cq:dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <Header
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Header"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <uniqueRefLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="uniqueRefLabel"
                                        name="./uniqueRefLabel"/>
                                    <placeholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="placeholder"
                                        name="./placeholder"/>
                                    <buttonLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="buttonLabel"
                                        name="./buttonLabel"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="description"
                                        name="./description"/>
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="title"
                                        name="./title"/>
                                    <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="image"
                                        name="./image"/>
                                    <richtext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="richtext"
                                        name="./richtext"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                </items>
                            </Header>
                            <Appointment3
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Appointment3"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <dateTitle2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="dateTitle2"
                                        name="./dateTitle2"/>
                                    <dateInfo2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="dateInfo2"
                                        name="./dateInfo2"/>
                                    <message2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="message2"
                                        name="./message2"/>
                                    <name2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="name2"
                                        name="./name2"/>
                                    <selectdate2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="selectdate2"
                                        name="./selectdate2"/>
                                    <icon2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="icon2"
                                        name="./icon2"/>
                                    <richtext3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="richtext3"
                                        name="./richtext3"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                </items>
                            </Appointment3>
                            <appointmentTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Appointment Details"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <appointmentTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter appointment title"
                                        fieldLabel="Appointment Title"
                                        name="./appointmentTitle"/>
                                    <appointmentDate
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter date (e.g., 05/12/2024)"
                                        fieldLabel="Appointment Date"
                                        name="./appointmentDate"/>
                                    <appointmentTime
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter time range (e.g., 08:00 - 17:00)"
                                        fieldLabel="Appointment Time"
                                        name="./appointmentTime"/>
                                    <appointmentReference
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter reference ID"
                                        fieldLabel="Unique Reference ID"
                                        name="./appointmentReference"/>
                                    <appointmentMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        emptyText="Enter survey message"
                                        fieldLabel="Survey Message"
                                        name="./appointmentMessage"/>
                                    <icon3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="icon3"
                                        name="./icon3"/>
                                    <icon4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="icon4"
                                        name="./icon4"/>
                                    <richtext4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="richtext4"
                                        name="./richtext4"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                </items>
                            </appointmentTab>
                            <Appointment2
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Appointment2"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <dateTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="dateTitle"
                                        name="./dateTitle"/>
                                    <dateInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="dateInfo"
                                        name="./dateInfo"/>
                                    <selectdate
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="selectdate"
                                        name="./selectdate"/>
                                    <message
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="message"
                                        name="./message"/>
                                    <name
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="name"
                                        name="./name"/>
                                    <icon
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="icon"
                                        name="./icon"/>
                                    <image3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="image3"
                                        name="./mage3"/>
                                    <image4
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="image4"
                                        name="./mage4"/>
                                    <richtext2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="richtext2"
                                        name="./richtext2"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                </items>
                            </Appointment2>
                            <Caution
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Caution"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <errorTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="errorTitle"
                                        name="./errorTitle"/>
                                    <retryButtonText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="retryButtonText"
                                        name="./retryButtonText"/>
                                    <errorDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="errorDescription"
                                        name="./errorDescription"/>
                                    <image7
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="image7"
                                        name="./image7"/>
                                </items>
                            </Caution>
                        </items>
                    </tabs>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
